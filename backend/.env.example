# PrintPro3D Backend Configuration
# Copy this file to .env and update with your values

# Server Configuration
PORT=8080
NODE_ENV=development

# Database Configuration (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=printpro3d
DB_USER=postgres
DB_PASSWORD=your_postgres_password

# For Google Cloud SQL (production)
# DB_HOST=/cloudsql/YOUR_PROJECT_ID:YOUR_REGION:YOUR_INSTANCE_NAME
# Or use Cloud SQL Proxy: DB_HOST=localhost

# Authentication
JWT_SECRET=your-secret-key-change-this-in-production

# Printer Backend API
PRINTER_BACKEND_URL=https://printer-backend-934564650450.europe-west1.run.app

# Google Cloud Storage (for GCODE files)
GCS_BUCKET_NAME=your-bucket-name
GOOGLE_CLOUD_PROJECT=your-project-id

# Optional: Logging
LOG_LEVEL=info
